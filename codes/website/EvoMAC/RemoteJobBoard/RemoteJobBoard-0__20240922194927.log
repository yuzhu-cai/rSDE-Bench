def read_users():
    users = []
    with open(USERS_FILE, 'r') as file:
        for line in file:
            users.append(line.strip().split(','))
    return users
def read_jobs():
    jobs = []
    with open(JOBS_FILE, 'r') as file:
        for line in file:
            jobs.append(line.strip().split(','))
    return jobs
