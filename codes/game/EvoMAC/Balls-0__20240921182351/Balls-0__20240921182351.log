    for ball in small_balls:
        if ball[2] != -1:  # Only draw if not consumed
            pygame.draw.circle(screen, YELLOW, (WIDTH//2 + ball[0], HEIGHT//2 + ball[1]), ball[2])
    pygame.display.flip()
    pygame.time.delay(30)
pygame.quit()
