'''
import pygame
import random
import json
import time
# Constants
WIDTH, HEIGHT = 800, 600
PLAYER_RADIUS = 20
ENEMY_RADIUS = 15
SPAWN_RADIUS = 5
LOG_FILE = "game.log"
# Initialize Pygame
pygame.init()
screen = pygame.display.set_mode((WIDTH, HEIGHT))
pygame.display.set_caption("Battle of Balls")
# Colors
