'''
import pygame
import random
import json
import time
# Constants
WIDTH, HEIGHT = 800, 600
PLAYER_RADIUS = 20
ENEMY_RADIUS = 15
SPAWN_RADIUS = 5
LOG_FILE = "game.log"
SPAWN_INTERVAL = 2000  # milliseconds
# Initialize Pygame
pygame.init()
screen = pygame.display.set_mode((WIDTH, HEIGHT))
pygame.display.set_caption("Battle of Balls")
# Colors
WHITE = (255, 255, 255)
RED = (255, 0, 0)
GREEN = (0, 255, 0)
BLUE = (0, 0, 255)
YELLOW = (255, 255, 0)
# Game state
player_pos = [0, 0]
player_radius = PLAYER_RADIUS
active_enemies = [[random.randint(-WIDTH//2, WIDTH//2), random.randint(-HEIGHT//2, HEIGHT//2), ENEMY_RADIUS] for _ in range(3)]
fixed_enemies = [[random.randint(-WIDTH//2, WIDTH//2), random.randint(-HEIGHT//2, HEIGHT//2), ENEMY_RADIUS]]
small_balls = []
# Log initialization
def log_event(event_type):
    timestamp = time.time()
    log_entry = {
        "timestamp": timestamp,
        "EVENT_TYPE": event_type,
        "game_state": {
            "player": {
                "position": player_pos,
                "radius": player_radius,
