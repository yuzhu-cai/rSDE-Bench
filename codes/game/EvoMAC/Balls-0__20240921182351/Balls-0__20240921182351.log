        # Setup initial game state
        self.player_pos = [0, 0]
        self.player_radius = 20
        self.active_enemies = [[-10, 0, 15], [10, 0, 15], [0, -10, 15]]
        self.fixed_enemies = [[0, 10, 15]]
        self.log_entries = []

