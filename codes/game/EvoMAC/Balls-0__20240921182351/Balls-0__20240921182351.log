                    enemy2[2] = -1  # Mark enemy2 as consumed
                else:  # enemy2 consumes enemy1
                    enemy2[2] += enemy1[2]
                    log_event("EAT_ENEMY")  # Log the event
                    enemy1[2] = -1  # Mark enemy1 as consumed
# Main game loop
running = True
while running:
    for event in pygame.event.get():
